#include <stdio.h>
int main(){
int cie,see,num;
printf("Enter your CIE mark ");
scanf("%d",&cie);
printf("Enter your SEE mark ");
scanf("%d",&see);
num=(((cie*2)+see)/20);

if(num >= 9){
	printf(" You got S grade"); 
		}
	else if(num >= 8){
	printf(" You got A grade"); 
		}
	else if ( num >=7){
		printf(" You got B grade");
		}
	else if ( num >=6){
		printf(" You got C grade");
		}
    else if ( num >=5){
		printf(" You got D grade");
		}
	else if ( num <= 4){
		printf(" You Failed in this exam");
		}
return 0;
}
